<div class="container">
    <div class="jumbotron text-center">
        <h1 class="display-4">Sign Up For Full Access!</h1>
    </div>
</div>

<div class="container-fluid">
	<div class="row">
		<div class="col-8 offset-2 text-center">
			<form class="signup-form">
				<div class="form-group"> 
					<label for="inputUsername"></label>
					<input type="input" class="form-control" autofocus="autofocus" placeholder="Create Username" id="inputUsername"/>
				</div>
				<div class="form-group">
					<label for="inputPassword"></label>
					<input type="password" class="form-control" placeholder="Create Password" id="inputPassword"/>
				</div>
                <button type="submit" class="btn btn-outline-dark" id="signupBtn">Submit</button><br>
			</form>
		</div>
	</div>
</div>